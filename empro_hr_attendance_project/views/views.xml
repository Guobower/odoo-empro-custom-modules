<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id="empro_hr_attendance_project_form_view" model="ir.ui.view">
      <field name="name">hr.attendance.form</field>
      <field name="model">hr.attendance</field>
      <field name="inherit_id" ref="hr_attendance.hr_attendance_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='check_out']" position="after">
          <field name="x_proyecto_asignado_id"/>
          <field name="x_fase_asignado_id"/>
          <field name="x_comentarios_del_dia_de_asistencia"/>
          <field name="x_biometrico"/>
          <field name="x_es_descanso"/>
          <field name="x_es_falta_justificada"/>
          <field name="x_es_falta_no_justificada"/>
          <field name="x_es_haber_basico"/>
          <field name="x_es_vacacion"/>
        </xpath>
      </field>
    </record>
    <record id="empro_hr_attendance_project_tree_view" model="ir.ui.view">
      <field name="name">hr.attendance.tree</field>
      <field name="model">hr.attendance</field>
      <field name="inherit_id" ref="hr_attendance.view_attendance_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='check_out']" position="after">
          <field name="x_proyecto_asignado_id"/>
          <field name="x_fase_asignado_id"/>
          <field name="x_comentarios_del_dia_de_asistencia"/>
          <field name="x_biometrico"/>
          <field name="x_es_descanso"/>
          <field name="x_es_falta_justificada"/>
          <field name="x_es_falta_no_justificada"/>
          <field name="x_es_haber_basico"/>
          <field name="x_es_vacacion"/>
        </xpath>
      </field>
    </record>
  </data>
</odoo>

 